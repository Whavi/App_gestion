{% extends "layouts/connected/baseConnected.html.twig" %}
{% block title %} Gestion de fourniture - SIF {% endblock %}



{% block body %}

<h2 class = "text-center mt-4"> Liste de fourniture dans la base de donnée

  <div>
    {% include 'layouts/connected/_searchbar.html.twig'with {
      form:form
    } %}
    </div>

    <table class="table table-dark table-bordered table-hover mt-4 w-75 mx-auto h5">
        <thead class= "table table-light">
          <tr>
            <th>Identifiant</th>
            <th>Nom</th>
            <th>Catégorie</th>
            <th>Action</th>
          </tr>
        </thead>
    {% for liste in listes %}
    <tr class = "text-center"> 
      <td>{{liste.identifiant}}</td> 
      <td>{{liste.nom}}</td> 
      <td>{{liste.category}}</td> 
    </tr>
    {% endfor %}
    </table>
    <div class = " navigation flex mt-4 justify-content-center mb-8">
      {{ knp_pagination_render(listes)}}
    </div>
{% endblock %}


