{% extends "layouts/connected/baseConnected.html.twig" %}
{% block title %} Gestion de fourniture - SIF {% endblock %}



{% block body %}
<h2 class = "text-center mt-4 "> Gestion des collaborateurs
    <table class="table table-dark mt-5 w-75 mx-auto h5">
        <thead>
          <tr class ="w-25" >
            <th class ="w-25">Nom</th>
            <th class ="w-25">Prenom</th>
            <th class ="w-25">Email</th>
            <th class ="w-25">Collaborateur</th>
          </tr>
        </thead>
    {% for collaborateur in collaborateurs %}
    <tr class = "text-center">
        <td>{{collaborateur.nom}}</td>
        <td>{{collaborateur.prenom}}</td>
        <td>{{collaborateur.email}}</td>
        <td>{{collaborateur.id}}</td>

    </tr>
    {% endfor %}
    </table>
{% endblock %}
